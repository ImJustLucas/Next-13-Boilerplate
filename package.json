    ],
    "**/*.{html}": [
      "npx prettier --write"
    ]
  },
  "dependencies": {
    "babel-plugin-styled-components": "^2.1.4",
    "dayjs": "^1.11.10",
    "next": "13.5.4",
    "react": "^18",
    "react-dom": "^18",
    "redaxios": "^0.5.1",
    "remixicon": "^3.5.0",
    "sonner": "^1.0.3",
    "styled-components": "^6.0.8",
    "typescript-plugin-styled-components": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.5",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
