    ],
    "**/*.{html}": [
      "npx prettier --write"
    ]
  },
  "dependencies": {
    "axios": "^1.6.5",
    "babel-plugin-styled-components": "^2.1.4",
    "dayjs": "^1.11.10",
    "next": "13.5.6",
    "react": "^18",
    "react-dom": "^18",
    "remixicon": "^4.2.0",
    "sonner": "^1.4.41",
    "styled-components": "^6.1.9",
    "typescript-plugin-styled-components": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.2.2",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
